web: java -Dserver.port=$PORT -jar web/target/web-0.0.1-SNAPSHOT-exec.jar
submitter: java -jar submission-submitter/target/submission-submitter-0.0.1-SNAPSHOT-exec.jar
checker: java -jar submission-checker/target/submission-checker-0.0.1-SNAPSHOT-exec.jar
combined: java -jar submission-submitter-and-checker/target/submission-submitter-and-checker-0.0.1-SNAPSHOT-exec.jar